            <%# JW - Added in link; for now directs to an index like page with all products by merchant, can eventually get rid of to directly go to merch page%>
            <li class='nav-item'><%= link_to merchants_path, method: :get, class: "nav-link" do %><i class="fas fa-users"></i> SHOP BY MERCHANT</li><%end%>
            <% if session[:merchant_id].nil? %>
            <li class='nav-item'><%= link_to github_login_path, class: "nav-link" do %> <i class="fas fa-sign-in-alt"></i> MERCHANT LOGIN</li><%end%>
            <% else %>
            <li class='nav-item'><%= link_to dashboard_path, class: "nav-link" do %><i class="fas fa-icons"></i> MY DASHBOARD</li><%end%>
            <li class='nav-item'><%= link_to logout_path, method: :post, class: "nav-link" do %><i class="fas fa-sign-out-alt"></i> MERCHANT LOGOUT</li><%end%>

            <% end %>
            <li class='nav-item'><%= link_to new_order_path, class: "nav-link" do %><i class="fas fa-shopping-cart"></i> CART <%= cart_num_items %> </li><%end%>
        </ul>
    </nav>
    <%= yield %>

