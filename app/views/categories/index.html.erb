<div class="row products-list">
<h1>Categories:</h1>

  <table class="table">
    <tbody>
      <% @categories.each do |category| %>
          <tr class="list-group-item-action">
            <td> <%= link_to category.name, category_path(category.id) %> </td>
          </tr>
      <% end %> 
    </tbody>
  </table>
</div>