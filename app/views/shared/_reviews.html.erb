<div class='card-deck'>
    <% review_set.each do |review| %>
        <div class="card">
          <div class="card-body">
              <p class="card-text">Rating: <%= review.rating %></p>
              <p class='card-text'>Comment: <%= review.review_text %></p>
          </div>
        </div>
    <% end %>
</div>