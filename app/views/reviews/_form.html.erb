


<%= form_with(model: review, local: true) do |form| %>
  <p>Please enter your review  </p>
  <div class='form-group'>
    <%= form.label :rating, class: 'control-label' %>
    <%= form.text_field :rating, class: 'form-control' %>
  </div>
 <div id="rateYo"></div>
  <div class='form-group'>
    <%= form.label :comment, class: 'control-label' %>
    <%= form.text_area :comment, class: 'form-control' %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>





  

