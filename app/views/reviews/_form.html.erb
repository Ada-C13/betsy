<section>
	<%= form_with model: @review do |f| %>
		<div class="form-group" >
			<%= f.label :category %>
			<%# f.collection_check_boxes(["1", "2"]) %>
		</div>
		<div class="form-group">
			<%= f.label :text %>
			<%= f.text_field :text, class: "form-control" %>
		</div>
		<div class="form-group">
			<%= f.submit class: "btn btn-primary" %>
		</div>
	<% end %>
</section>
