<h2><%= list_name %></h2>
<h4><%= "#{variable.count} products" %></h4>
<main class='product-index'> 
<% variable.each do |product| %>
  <section class="product_square">

    <%= image_tag product.img_url, alt: product.description %>
    <h4><%= link_to product.name, product_path(product.id) %></h4>
    <h5><%# product.description %></h5>
    <h6><%= product.price %></h6>
    
  </section>
<% end %>
</main> 
